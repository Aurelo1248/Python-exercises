   "source": [
    "def iter_even():\n",
    "    i = 0\n",
    "    while True:\n",
    "        yield i*2\n",
    "        i += 1\n",
    "\n",
    "def iter_odd():\n",
    "    i = 1\n",
    "    while True:\n",
    "        yield i\n",
    "        i += 2\n",
    "\n",
    "def iter_sqr():\n",
    "    n=2\n",
    "    i = 0\n",
    "    while True:\n",
    "        yield n**i\n",
    "        i = 1+i"
